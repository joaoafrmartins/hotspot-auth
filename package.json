{
  "name": "hotspot-auth",
  "version": "0.0.0",
  "description": "Hotspot Auth",
  "keywords": [
    "hotspot",
    "auth"
  ],
  "scripts": {
    "start": "a-http-server",
    "commit": "spaghetty github --commit --recursive"
  },
  "dependencies": {
    "a-http-server": "",
    "a-http-server-plugin-api": "",
    "a-http-server-component-api-user": "",
    "a-http-server-component-auth": ""
  },
  "devDependencies": {},
  "config": {
    "port": 3000,
    "middleware": {
      "cookies": {
        "secret": "secret"
      }
    },
    "components": {
      "a-http-server-component-auth": {
        "facebook": {
          "enabled": true,
          "appId": "",
          "appSecret": "",
          "callbackUrl": ""
        },
        "twitter": {
          "enabled": false,
          "appId": ""
        }
      }
    },
    "plugins": {
      "a-http-server-plugin-session": {
        "redis": {
          "socket": "./data/redis/redis-server.sock"
        }
      },
      "a-http-server-plugin-api": {
        "endpoint": "/api/v1",
        "database": {
          "adapter": "redis",
          "options": {
            "socket": "./data/redis/redis-server.sock"
          }
        }
      }
    }
  },
  "homepage": "https://github.com/joaoafrmartins/hotspot-auth",
  "repository": {
    "type": "git",
    "url": "https://github.com/joaoafrmartins/hotspot-auth.git"
  },
  "bugs": {
    "url": "https://github.com/joaoafrmartins/hotspot-auth/issues"
  }
}
